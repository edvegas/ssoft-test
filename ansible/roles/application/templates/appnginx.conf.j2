server {
    listen 80;
    server_name localhost;
    access_log /var/log/nginx/access.log;
    error_log /var/log/nginx/error.log;

    location / {
        root {{ homedir }}/test-app/app/static;
    }

    location /api/ {
        include uwsgi_params;
        uwsgi_pass unix://{{ homedir }}/uwsgi.sock;
    }
}
